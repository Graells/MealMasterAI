{
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true}
  },
  "rules": {   
     "react/jsx-uses-react": "error",   
     "react/jsx-uses-vars": "error" 
},
  "env": {
    "es6": true,
    "browser": true,
    "node": true
	}
}